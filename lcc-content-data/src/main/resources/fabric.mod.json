{
  "schemaVersion": 1,
  "id": "lcc-content-data",
  "version": "${version}",

  "name": "Loosely Connected Concepts - Content Datagen",
  "description": "This module is responsible for generating the data of Loosely Connected Concepts.",
  "authors": [
    "joshmanisdabomb"
  ],
  "contact": {
    "homepage": "https://joshmanisdabomb.com/",
    "sources": "https://bitbucket.org/joshmanisdabomb/loosely-connected-concepts/src/fabric/"
  },

  "license": "All rights reserved, see LICENSE.md",
  "icon": "assets/lcc-content-data/icon.png",

  "environment": "*",
  "entrypoints": {
    "preLaunch": [
      {
        "adapter": "kotlin",
        "value": "com.joshmanisdabomb.lcc.data.LCCData"
      }
    ]
  },
  "depends": {
    "fabricloader": ">=0.11.1",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.4.21",
    "minecraft": "1.19.x",
    "lcc": ["0.5.1", "${version}"]
  },
  "custom": {
    "modmenu": {
      "parent": "lcc-mod",
      "badges": [ "library" ]
    }
  }
}
