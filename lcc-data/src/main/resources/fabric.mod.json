{
  "schemaVersion": 1,
  "id": "lcc-data",
  "version": "${version}",

  "name": "Loosely Connected Concepts - Data Generator",
  "description": "This module is responsible for generating the data of Loosely Connected Concepts.",
  "authors": [
    "joshmanisdabomb"
  ],
  "contact": {
    "homepage": "https://joshmanisdabomb.com/",
    "sources": "https://bitbucket.org/joshmanisdabomb/loosely-connected-concepts/src/fabric/"
  },

  "license": "All rights reserved, see LICENSE.md",
  "icon": "assets/lcc-multipart/icon.png",

  "environment": "*",
  "entrypoints": {
    "preLaunch": [
      {
        "adapter": "kotlin",
        "value": "com.joshmanisdabomb.lcc.LCCData"
      }
    ]
  },
  "mixins": [
    "lcc-data.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.1",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.4.21",
    "minecraft": "1.17.x",
    "lcc": ["0.4.2", "${version}"]
  },
  "custom": {
    "modmenu:api": true,
    "modmenu:parent": "lcc-mod"
  },
  "accessWidener": "lcc-data.accesswidener"
}
